#include "holberton.h"
/**
 * _strncpy - copies a string
 * @dest: destination string
 * @src: source string
 * @n: length of appended bytes
 * Return: dest string
 */
char *_strncpy(char *dest, char *src, int n)
{
	int write, read = 0;

	for (; write < n; write++, read++)
		dest[write] = src[read];
	return (dest);
}
